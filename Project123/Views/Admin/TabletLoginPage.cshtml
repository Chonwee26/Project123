@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>DateTimePicker - Month and Year</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css">
    <link rel="stylesheet" href="/lib/boostarp-datepicker/dist/css/bootstrap-datepicker.min.css">
    @*<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.9.0/css/bootstrap-datepicker.min.css">*@
    <script src="https://code.jquery.com/jquery-3.5.1.min.js"></script>
    <script src="/lib/boostarp-datepicker/dist/js/bootstrap-datepicker.min.js"></script>
    @*<script src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.9.0/js/bootstrap-datepicker.min.js"></script>*@
</head>
<body>
    <div class="container mt-5">
        <input type="text" class="form-control" id="datepicker">
    </div>

    <script>
        $(function () {
            $('#datepicker').datepicker({
                format: "mm/yyyy",
                startView: "months",
                minViewMode: "months",
                autoclose: true
            });
        });
    </script>
</body>
</html>
